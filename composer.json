{
	"name": "rkr/view",
	"description": "More secure and easy to use templating system for php5.4+",
	"license": "MIT",
	"require": {
		"php": ">=5.4"
	},
	"require-dev": {
		"phpunit/phpunit": "~3.7@stable"
	},
	"autoload": {
		"psr-4": {
			"View\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"View\\": ["src/", "tests/"]
		}
	}
}
